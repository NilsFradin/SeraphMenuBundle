{{ form_start(form) }}
<div class="card-body">
    <div class="form-group">
        <label for="menu_name">{{ form_label(form.name) }}</label>
        {{ form_widget(form.name) }}
    </div>
    <div class="form-group">
        <label for="menu_slug">{{ form_label(form.slug) }}</label>
        {{ form_widget(form.slug) }}
    </div>
</div>
<div class="card-footer">
    <button class="btn btn-sm btn-outline-success" type="submit">
        New
    </button>
    <button class="btn btn-sm btn-outline-danger btn-reset" type="reset">
        Reset
    </button>
    <a class="btn btn-sm btn-outline-gray-600" href="{{ path('fr-menu-list-page') }}">
        Back
    </a>
</div>
{{ form_end(form) }}